[nginx_proxy]
nginx-dev ansible_host=${nginx_dev_private_ip}

[frontend]
frontend-dev ansible_host=${frontend_dev_private_ip}

[backend]
backend-dev ansible_host=${backend_dev_private_ip}

[db]
db-dev ansible_host=${db_dev_private_ip}

[devops]
devops-dev ansible_host=${devops_dev_private_ip}

[all:vars]
ansible_python_interpreter=/usr/bin/python3
