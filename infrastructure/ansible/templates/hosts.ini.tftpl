[nginx_proxy]
${nginx_vm_name} ansible_host=${nginx_vm_name} ansible_user=$${ANSIBLE_USER}

[frontend]
${frontend_vm_name} ansible_host=${frontend_vm_name} ansible_user=$${ANSIBLE_USER}

[backend]
${backend_vm_name} ansible_host=${backend_vm_name} ansible_user=$${ANSIBLE_USER}

[db]
${db_vm_name} ansible_host=${db_vm_name} ansible_user=$${ANSIBLE_USER}

[devops]
${devops_vm_name} ansible_host=${devops_vm_name} ansible_user=$${ANSIBLE_USER}

[all:vars]
ansible_python_interpreter=/usr/bin/python3
